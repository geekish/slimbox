{
    "name": "geekish/slimbox",
    "type": "library",
    "description": "Slim Framework with Unbox container",
    "keywords": [
        "geekish",
        "slimbox"
    ],
    "homepage": "https://github.com/geekish/slimbox",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannah Warmbier",
            "email": "hannahwarmbier@gmail.com",
            "homepage": "https://github.com/geekish",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "mindplay/unbox": "^2.0",
        "slim/slim": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Geekish\\Slimbox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geekish\\Slimbox\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
